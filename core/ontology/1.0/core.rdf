<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="https://w3id.org/arm/core/ontology/1.0/" xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:vann="http://purl.org/vocab/vann/"
	xmlns:voaf="http://purl.org/vocommons/voaf#">
	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // ONTOLOGY DECLARATION
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
	<owl:Ontology rdf:about="https://w3id.org/arm/core/ontology/">
		<owl:versionIRI rdf:resource="https://w3id.org/arm/core/ontology/1.0/"/>
		<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version
			1.0</owl:versionInfo>
		<dcterms:issued>2020-11-11T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2010-11-11T00:00:00-04:00</dcterms:modified>
		<rdfs:label xml:lang="en">Art and Rare Materials Core Ontology</rdfs:label>
		<skos:definition xml:lang="en">The Art and Rare Materials Core Ontology provides specialized
			modeling of bibliographic metadata in the art and rare materials domains. It is an
			extension to the BIBFRAME ontology for general bibliographic
			description.</skos:definition>
		<vann:preferredNamespacePrefix>arm</vann:preferredNamespacePrefix>
		<dcterms:title xml:lang="en">Art and Rare Materials Core Ontology</dcterms:title>
		<voaf:extends rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
		<voaf:specializes rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
	</owl:Ontology>
	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // CLASSES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
	<!-- 
new in 1.0:
 arm:Annotation
 arm:Bookplate
 arm:Event
 arm:ItemHistory
 -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Annotation">
		<rdfs:label xml:lang="en">Annotation</rdfs:label>
		<skos:definition xml:lang="en">Remarks, notes, highlighting, or symbols that describe,
			explain, evaluate, or otherwise reflect an agent’s interaction with an object or that
			document the history or provenance of the object.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2020-11-14T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2020-11-14T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Bookplate">
		<rdfs:label xml:lang="en">Bookplate</rdfs:label>
		<skos:definition xml:lang="en">Labels affixed to a book to indicate ownership or used for
			identification. They may be printed or engraved, frequently with a distinctive design,
			tipped in or pasted on the inside of the upper or lower cover or
			flyleaf.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2020-11-14T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2020-11-14T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Event">
		<rdfs:label xml:lang="en">Event</rdfs:label>
		<skos:definition xml:lang="en">Optional mechanism to provide granular documentation of
			events on resources. May include conservation activities, exhibits, provenance
			information, or physical condition.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/ItemHistory"/>
		<dcterms:issued>2020-11-14T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2020-11-14T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/ItemHistory">
		<rdfs:label xml:lang="en">Item History</rdfs:label>
		<skos:definition xml:lang="en">Optional superclass to gather all events for a resource
			together as needed.</skos:definition>
		<dcterms:issued>2020-11-14T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2020-11-14T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AccessionNumber">
		<rdfs:label xml:lang="en">Accession number</rdfs:label>
		<skos:definition xml:lang="en">Numeric, alphanumeric, or other identifying codes assigned
			when an art object, book, or other item enters the collection of a museum, library, or
			other repository. Such codes are unique within the set of codes, and specifically
			identify the particular item at hand. The numbers may be marked on the objects or not. </skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
		<skos:editorialNote xml:lang="en">Deprecate this class when it is defined in
			BIBFRAME.</skos:editorialNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Attribution">
		<rdfs:label xml:lang="en">Attribution</rdfs:label>
		<skos:definition xml:lang="en">The ascribing of an activity to a particular agent or agents.
			This attribution may change over time based on new evidence.</skos:definition>
		<skos:scopeNote xml:lang="en">The Attribution is directly related to an Activity, and
			carries additional data about the agent, source, date, etc. of the
			attribution.</skos:scopeNote>
		<skos:example xml:lang="en">The activity of creating an artwork is ascribed to a particular
			artist.</skos:example>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Autograph">
		<rdfs:label xml:lang="en">Autograph</rdfs:label>
		<skos:definition xml:lang="en">A person's name written on a resource in their own hand.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<skos:example xml:lang="en">A binder's signature</skos:example>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/BindersTicket">
		<rdfs:label xml:lang="en">Binder's ticket</rdfs:label>
		<skos:definition xml:lang="en">Small engraved or printed label, usually found on the upper
			outside corner of one of the front flyleaves, giving the name and usually the address of
			the bookbinder; common in the 18th and early 19th centuries.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Binding">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Enclosure"/>
		<rdfs:label xml:lang="en">Binding</rdfs:label>
		<skos:definition xml:lang="en">Covering of a book which holds the sheets together to provide
			protection, and collectively all the parts of a book applied in the process of attaching
			the leaves and covering them.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/BoundCollection">
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Item"/>
		<rdfs:label xml:lang="en">Bound collection</rdfs:label>
		<skos:definition xml:lang="en">An artificial gathering of two or more items physically into
			or on a single carrier, whether bound, affixed, or otherwise brought together into a
			single bound unit, and lacking a collective title. Includes only collections gathered
			subsequent to publication, rather than issued or reissued together by the
			publisher.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/CaptionNote">
		<rdfs:label xml:lang="en">Caption note</rdfs:label>
		<skos:definition xml:lang="en">A distinctive caption independent of the resource title,
			including mottos and headings.</skos:definition>
		<rdfs:comment xml:lang="en">Use separate notes for multiple captions.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Citation">
		<rdfs:label xml:lang="en">Citation</rdfs:label>
		<skos:definition xml:lang="en">A single citation within a bibliographic
			source.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/ConditionAssessment">
		<rdfs:label xml:lang="en">Condition assessment</rdfs:label>
		<skos:definition xml:lang="en">Assessment and/or documentation of the physical condition of
			an item, including damage, material vulnerabilities, special storage requirements, etc.
			The assessment is generally the output of a condition assessment activity, and may or
			may not result in a conservator activity.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/ContentsNote">
		<rdfs:label xml:lang="en">Contents note</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<skos:definition xml:lang="en">A note to provide a main contents of the resource rather than
			linking to an external table of contents or creating separate part/whole relationships
			to the entities that comprise the whole. Ideally, part whole relationships would be
			created instead of the this type of note.</skos:definition>
		<skos:editorialNote>This note type was created based on
			http://bibliotek-o.org/1.1/ontology/specifyingContents. Likely belongs in a general
			bf:Notes extension, but need to add it here for now.</skos:editorialNote>
		<dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/DescriptiveNote">
		<rdfs:label xml:lang="en">Descriptive note</rdfs:label>
		<skos:definition xml:lang="en">A note that describes the resource.</skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Enclosure">
		<rdfs:label xml:lang="en">Enclosure</rdfs:label>
		<skos:definition xml:lang="en">Covering or container to provide protection, identification, and/or keep constitutent parts together.</skos:definition>
		<skos:scopeNote xml:lang="en">This class includes containers, such as a box containing a
			paper doll set.</skos:scopeNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Entry">
		<rdfs:label xml:lang="en">Entry</rdfs:label>
		<skos:definition xml:lang="en">A specific entry within a bibliographic
			resource.</skos:definition>
		<skos:example xml:lang="en">An entry locating a citation within a bibliographic
			source.</skos:example>
		<skos:example xml:lang="en">An exhibition catalog entry describing a specific item in the
			exhibition.</skos:example>
		<skos:scopeNote xml:lang="en">Used in parsing out hierarchically structured location
			designators for citations.</skos:scopeNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Exhibition">
		<rdfs:label xml:lang="en">Exhibition</rdfs:label>
		<skos:definition xml:lang="en">The relationship between an ExhibitionEvent and an Item in
			that event.</skos:definition>
		<skos:scopeNote xml:lang="en">Used to attach information to this relationship, such as
			title, dates, activities such as curatorship, exhibit number, physical arrangement, etc.
			which does not properly attach to the Item in and of itself.</skos:scopeNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Font">
		<rdfs:label xml:lang="en">Font</rdfs:label>
		<skos:definition xml:lang="en">A font is the digital representation of a typeface. It is a
			collection of all the characters of a typeface in one size (12pt/1in) and one style
			(bold/italic). For example, Arial in 12pt size is a font, Arial in 18pt size is a font,
			and Arial bold in 24pt is also a font.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/FontStyle">
		<rdfs:label xml:lang="en">Font style</rdfs:label>
		<skos:definition xml:lang="en">Style of the font, e.g. bold, italic</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Frame">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Mount"/>
		<rdfs:label xml:lang="en">Frame</rdfs:label>
		<skos:definition xml:lang="en">A supportive structure that surrounds or encloses an
			item.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Handwriting">
		<rdfs:label xml:lang="en">Handwriting</rdfs:label>
		<skos:definition xml:lang="en">A specific example of a handwriting found in a textual
			resource. A handwriting can have a particular HandWritingType.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/HandwritingType">
		<rdfs:label xml:lang="en">Handwriting type</rdfs:label>
		<skos:definition xml:lang="en">A type of writing hand; not the same as a writing system or a
			handwriting style (such as cursive or block letters). Examples include Gothic,
			Secretary, Carolingian minuscule, and Sütterlin.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/InaccuracyNote">
		<rdfs:label xml:lang="en">Inaccuracy note</rdfs:label>
		<skos:definition xml:lang="en">A note explaining an unintentional or intentional inaccuracy,
			misspelling, or misprint relating to a specific resource.</skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Inscription">
		<rdfs:label xml:lang="en">Inscription</rdfs:label>
		<skos:definition xml:lang="en">Words, texts, lettering or symbols written or engraved on a
			resource, often reflecting commemoration or presentation.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Label">
		<rdfs:label xml:lang="en">Label</rdfs:label>
		<skos:definition xml:lang="en">A piece of paper, leather, fabric, or other material
			inscribed, printed, or engraved with information and affixed to a resource or part of a
			resource for identification or description.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/LimitationNote">
		<rdfs:label xml:lang="en">Limitation note</rdfs:label>
		<skos:definition xml:lang="en">A note relating to the size and nature of a print run limited
			to a specific quantity of copies produced, or a statement relating to the enumeration
			and nature of a copy in relation to the larger print run.</skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Marking">
		<rdfs:label xml:lang="en">Marking</rdfs:label>
		<skos:definition xml:lang="en">A symbol, notation, or other mark present in or applied to an
			object during or after creation.</skos:definition>
		<skos:editorialNote xml:lang="en">As currently defined, contrasts with marginialia and other
			unofficial markings applied by individual users outside the creation or production
			process or custodial history of an object. This distinction may at times be subtle, and
			is subject to future review.</skos:editorialNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Mount">
		<rdfs:label xml:lang="en">Mount</rdfs:label>
		<skos:definition xml:lang="en">An item placed on a pedestal or a case, or otherwise attached
			to a print, photographic image, or document protected by framing, usually against a
			backing material.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Page">
		<rdfs:label xml:lang="en">Page</rdfs:label>
		<skos:definition xml:lang="en">A single page within a resource.</skos:definition>
		<rdfs:comment xml:lang="en">Used in any page-level description, as well as in parsing out
			hierarchically structured location designators for citations.</rdfs:comment>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/PaginationFoliation">
		<rdfs:label xml:lang="en">Pagination foliation</rdfs:label>
		<skos:definition xml:lang="en">Sequential numbering of individual leaves, plates, pages
			(numbering on both sides), or columns (used as a sequence of numbering) of a
			resource.</skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Extent"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Pedestal">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Mount"/>
		<rdfs:label xml:lang="en">Pedestal</rdfs:label>
		<skos:definition xml:lang="en">A solid, fixed base or support on which an item or part of an
			item is mounted.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/PhysicalCondition">
		<rdfs:label xml:lang="en">Physical Condition</rdfs:label>
		<skos:definition xml:lang="en">One specific aspect of the condition of an object or part of
			an object, including perceptible or measurable characteristics such as physical
			appearance, level of damage, quality, or completeness.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/PreferredCitation">
		<rdfs:label xml:lang="en">Preferred citation</rdfs:label>
		<skos:definition xml:lang="en">A note providing a specific citation format for citing the
			resource.</skos:definition>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/RelatedMaterialNote">
		<rdfs:label xml:lang="en">Related material note</rdfs:label>
		<skos:definition xml:lang="en">A note providing information on materials (e.g. archival
			materials or publications) related to the described resource.</skos:definition>
		<skos:scopeNote xml:lang="en">Use if information describing the related material cannot be
			parsed into the appropriate relationship.</skos:scopeNote>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Seal">
		<rdfs:label xml:lang="en">Seal</rdfs:label>
		<skos:definition xml:lang="en">An impression that has been made in wax, clay, or another
			soft material by the impact of a hard engraved surface with monograms, writing, numbers,
			symbols, or design in intaglio, generally used to authenticate documents or for
			decoration.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/SignatureStatement">
		<rdfs:label xml:lang="en">Signature statement</rdfs:label>
		<skos:definition xml:lang="en">A formulaic statement describing the pattern followed in
			assembling the sheets of the book, as expressed in its signatures. Signatures are
			letters, numerals, symbols, or a group of such characters printed at the foot of the
			rectos of the first few leaves of each gathering (quire) to aid the binder in correctly
			assembling the sections.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Stamp">
		<rdfs:label xml:lang="en">Stamp</rdfs:label>
		<skos:definition xml:lang="en">A mark that is impressed, printed, perforated, or embossed,
			made by a hard object, especially a die, block, or other tool, on a softer material such
			as paper or wax.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/StylePeriod">
		<rdfs:label xml:lang="en">Style/Period</rdfs:label>
		<skos:definition xml:lang="en">A defined style, historical or artistic period, movement,
			group, or school whose characteristics are represented in a work.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/SubjectNote">
		<rdfs:label xml:lang="en">Subject note</rdfs:label>
		<skos:definition xml:lang="en">Textual information describing the subject or iconographic
			details of a resource.</skos:definition>
		<skos:scopeNote xml:lang="en">Use for textual notes only, not for subject terms from a
			controlled value vocabulary.</skos:scopeNote>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/SummaryNote">
		<rdfs:label xml:lang="en">Summary note</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<skos:definition xml:lang="en">A note providing a summary of the resource.</skos:definition>
		<skos:editorialNote>This note type was created based on
			http://bibliotek-o.org/1.1/ontology/summarizing. Likely belongs in a general bf:Notes
			extension, but need to add it here for now.</skos:editorialNote>
		<dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/SupplementaryContentNote">
		<rdfs:label xml:lang="en">Supplementary content note</rdfs:label>
		<rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Note"/>
		<skos:definition xml:lang="en">A note indicating the existence of supplementary content such
			as bibliographies, discographies, filmographies, and/or other bibliographic references
			in the described resource or in accompanying material.</skos:definition>
		<skos:editorialNote>This note type was created based on
			http://bibliotek-o.org/1.1/ontology/describingSupplementaryContent. Likely belongs in a
			general bf:Notes extension, but need to add it here for now.</skos:editorialNote>
		<dcterms:issued>2018-01-29T00:00:00-04:00</dcterms:issued>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/TextBlock">
		<owl:disjointWith rdf:resource="https://w3id.org/arm/core/ontology/1.0/Enclosure"/>
		<rdfs:label xml:lang="en">Text block</rdfs:label>
		<skos:definition xml:lang="en">Those parts of a book consisting of the stack of leaves or
			gatherings to be bound, exclusive of the materials, such as endpapers or boards, that
			are added in binding.</skos:definition>
		<skos:scopeNote xml:lang="en">The measurements of a book may include the binding or not. A
			text block resource can be used in the specification of measurements exclusive of the
			binding.</skos:scopeNote>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Typeface">
		<rdfs:label xml:lang="en">Typeface</rdfs:label>
		<skos:definition xml:lang="en">A design or “font family” created by a typographer or type
			designer. It defines specific stylistic features to determine the type display of a
			writing system.</skos:definition>
		<rdfs:comment xml:lang="en">BIBFRAME uses bf:Script to refer to writing
			systems</rdfs:comment>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Volume">
		<rdfs:label xml:lang="en"/>
		<skos:definition xml:lang="en">A single bibliographic or physical volume of a
			resource.</skos:definition>
		<rdfs:comment xml:lang="en">Used in any volume-level description, as well as in parsing out
			hierarchically structured location designators for citations.</rdfs:comment>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Watermark">
		<rdfs:label xml:lang="en">Watermark</rdfs:label>
		<skos:definition xml:lang="en">A faint design or device incorporated into paper as it is
			made and appearing as translucent or shaded areas visual only through transmitted light.
			Often used to give the name of the manufacturer and the date of the paper's manufacture;
			a symbol, seal, or initials are also common. Often used to detect and prevent
			counterfeiting and forgery.</skos:definition>
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Marking"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardCitation -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardCitation">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is citation.</skos:definition>
		<rdfs:label>Award citation</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardGranterActivity -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardGranterActivity">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Activity"/>
		<skos:definition>The activity of granting an award.</skos:definition>
		<skos:scopeNote>This class is not derived from a MARC relator.</skos:scopeNote>
		<rdfs:label>Award granter</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardHonoraryMention -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardHonoraryMention">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is honorary
			mention.</skos:definition>
		<rdfs:label>Award honorary mention</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardLonglist -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardLonglist">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is longlist.</skos:definition>
		<rdfs:label>Award longlist</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardNominee -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardNominee">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is nominee.</skos:definition>
		<rdfs:label>Award nominee</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardReceipt -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardReceipt">
		<skos:definition>The bestowal of an award, honor, or distinction to an Agent or Work. The
			award bestowed should be represented with the Award class. The AwardReceipt is a context
			node between the award and the recipient which may contain further information, such as
			date.</skos:definition>
		<rdfs:label>Award receipt</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardSelectorActivity -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardSelectorActivity">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/Activity"/>
		<skos:definition>The activity of nominating or judging a resource in relation to an award,
			honor, etc.</skos:definition>
		<skos:scopeNote>This class is not derived from a MARC relator.</skos:scopeNote>
		<rdfs:label>Award selector</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardShortlist -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardShortlist">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is award
			shortlist.</skos:definition>
		<rdfs:label>Award shortlist</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- https://w3id.org/arm/core/ontology/1.0/AwardWinner -->
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/AwardWinner">
		<rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/AwardReceipt"/>
		<skos:definition>The specific nature of the award receipt is award winner.</skos:definition>
		<rdfs:label>Award winner</rdfs:label>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/Measurement">
		<rdfs:label>Measurement</rdfs:label>
		<skos:definition xml:lang="en-us">The measurement of a single aspect of a resource,
			including value, units, and the dimension measured. For example, a book may have a
			height (aspect) of 10 (value) centimeters (units). Each such measurement is attached to
			a MeasurementGroup.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<owl:Class rdf:about="https://w3id.org/arm/core/ontology/1.0/MeasurementGroup">
		<rdfs:label>Measurement Group</rdfs:label>
		<skos:definition xml:lang="en-us">A set of measurements pertaining to a specific resource,
			part of a resource, or resource in a particular arrangement. For example, a book may
			have a specified height, width, length, and/or weight, which are all attached to a
			MeasurementGroup; the binding may have measurements specified independently, which would
			constitute another MeasurementGroup. A MeasurementGroup has one or more specific
			Measurements attached to it.</skos:definition>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:Class>
	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // OBJECT PROPERTIES
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/agentOf">
		<rdfs:label xml:lang="en">agent of</rdfs:label>
		<skos:definition xml:lang="en">Relates an agent to the activity it participated
			in.</skos:definition>
		<rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Agent"/>
		<owl:inverseOf rdf:resource="http://id.loc.gov/ontologies/bibframe/agent"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/atLocation">
		<rdfs:label xml:lang="en">at location</rdfs:label>
		<skos:definition xml:lang="en">The resource being described is at the specified
			location.</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/isLocationOf"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/carries">
		<rdfs:label xml:lang="en">carries</rdfs:label>
		<skos:definition xml:lang="en">A particular physical form, such as a volume, a sheet of
			paper, or a computer disk, contains or has on it the resource being
			described.</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/isOnCarrier"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/cites">
		<rdfs:label xml:lang="en">cites</rdfs:label>
		<skos:definition xml:lang="en">Visual citation is an artistic process/practice in which a
			creator reproduces the totality or a section of a (well-known) work of art (here, the
			object) to incorporate it a new visual creation (here, the subject).</skos:definition>
		<rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/citedBy"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/citedBy">
		<rdfs:label xml:lang="en">cited by</rdfs:label>
		<skos:definition xml:lang="en">Visual citation is an artistic process/practice in which a
			creator reproduces the totality or a section of a (well-known) work of art (here, the
			subject) to incorporate it into a new visual creation (here, the
			object).</skos:definition>
		<rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/cites"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/coveredIn">
		<rdfs:label xml:lang="en">covered in</rdfs:label>
		<skos:definition xml:lang="en">Specifies a resource whose content covers this time period or
			location.</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/covers"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/covers">
		<rdfs:label xml:lang="en">covers</rdfs:label>
		<skos:definition xml:lang="en">Specifies a time period or location covered in the content of
			this resource.</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/coveredIn"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/describedBy">
		<rdfs:label xml:lang="en">described by</rdfs:label>
		<skos:definition xml:lang="en">This resource is described by another, for example a
			PhysicalCondition is described by a ConditionAssessment</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/describes"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/describes">
		<rdfs:label xml:lang="en">describes</rdfs:label>
		<skos:definition xml:lang="en">This resource provides a description of another, for example
			a ConditionAssessment describes a PhysicalCondition.</skos:definition>
		<owl:inverseOf rdf:resource="https://w3id.org/arm/core/ontology/1.0/describedBy"/>
		<dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
		<dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
	</owl:ObjectProperty>
	<owl:ObjectProperty rdf:about="https://w3id.org/arm/core/ontology/1.0/hasArrangement">
		<rdfs:label xml:lang="en">has arrangement</rdfs:label>
		<skos:definition xml:lang="en">Relates an object or collection of objects to information
			about its organization or arrangement.</skos:definition>
