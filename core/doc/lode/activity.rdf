        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CollectorActivity">
        <rdfs:label xml:lang="en">Collector</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of bringing together items from various sources that are then arranged, described, and cataloged as a collection. A collector is neither the creator of the material nor a person to whom manuscripts in the collection may have been addressed.</rdfs:comment>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/col.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CollotyperActivity">
        <rdfs:label xml:lang="en">Collotyper</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of manufacturing a manifestation of photographic prints from film or other colloid that has ink-receptive and ink-repellent surfaces.</rdfs:comment>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/clt.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/ColoristActivity">
        <rdfs:label xml:lang="en">Colorist</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of applying color to drawings, prints, photographs, maps, moving images, etc.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/clr.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CompilerActivity">
        <rdfs:label xml:lang="en">Compiler</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of creating a new work (e.g., a bibliography, a directory) through the act of compilation, e.g., selecting, arranging, aggregating, and editing data, information, etc.</rdfs:comment>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/com.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/ConditionAssessmentActivity">
        <rdfs:label xml:lang="en">Condition assessment activity</rdfs:label>
        <rdfs:comment xml:lang="en">The activity of assessing and documenting the physical condition of an item, including damage, material vulnerabilities, special storage requirements, etc. This activity outputs a ConditionAssessment.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/ConservatorActivity">
        <rdfs:label xml:lang="en">Conservator</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of preserving or treating printed or manuscript material, works of art, artifacts, or other objects.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/con.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/ContributorActivity">
        <rdfs:label xml:lang="en">Contributor</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of making contributions to the resource. This includes those whose work has been contributed to a larger work, such as an anthology, serial publication, or other compilation of individual works. If a more specific role is available, prefer that activity type, e.g. EditorActivity, CompilerActivity, IllustratorActivity.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/ctb.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CopyrightHolderActivity">
        <rdfs:label xml:lang="en">Copyright holder</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of receiving copy and legal rights have been granted or transferred for the intellectual content of a work. The copyright holder, although not necessarily the creator of the work, usually has the exclusive right to benefit financially from the sale and use of the work to which the associated copyright protection applies.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/cph.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CorrespondentActivity">
        <rdfs:label xml:lang="en">Correspondent</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of either the writer or recipient of a letter or other communication.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/crp.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CreatorActivity">
        <rdfs:label xml:lang="en">Creator</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of creating a resource.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/cre.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CuratorActivity">
        <rdfs:label xml:lang="en">Curator</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of conceiving, aggregating, and/or organizing an exhibition, collection, or other item.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/cur.</skos:scopeNote>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

