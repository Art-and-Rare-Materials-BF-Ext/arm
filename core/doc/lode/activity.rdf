    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/BinderActivity">
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <rdfs:label xml:lang="en">Binder</rdfs:label>
        <rdfs:comment xml:lang="en">The activity of attaching a Binding to a resource.</rdfs:comment>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">Both instances and items can have Bindings, but generally there is need to describe this activity only in relation to an item</skos:scopeNote>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/bnd.</skos:scopeNote>
    </owl:Class>
    
    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/CartographerActivity">
        <rdfs:label xml:lang="en">Cartographer</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of creating a map, atlas, globe, or other cartographic work.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="https://w3id.org/arm/core/activity/0.1/Activity"/>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/ctg.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
        <dcterms:issued>2018-04-27T00:00:00-04:00</dcterms:issued>
        <dcterms:modified>2018-04-27T00:00:00-04:00</dcterms:modified>
    </owl:Class>

    <owl:Class rdf:about="https://w3id.org/arm/core/activity/0.1/ChoreographerActivity">
        <rdfs:label xml:lang="en">Choreographer</rdfs:label>    
        <rdfs:comment xml:lang="en">The activity of creating or contributing to a work of movement.</rdfs:comment>
        <skos:scopeNote xml:lang="en">This class is derived from the MARC relator: http://id.loc.gov/vocabulary/relators/chr.</skos:scopeNote>
        <skos:editorialNote xml:lang="en">Future work: consider more formal alignment between the class and corresponding MARC relator.</skos:editorialNote>
