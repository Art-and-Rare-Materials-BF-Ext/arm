<?xml version="1.0"?>
<rdf:RDF xmlns="http://award.bibliotek-o.org/ontology/"
     xml:base="http://award.bibliotek-o.org/ontology/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:voaf="http://purl.org/vocommons/voaf#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    >
  
    <!-- ONTOLOGY DECLARATION -->
    
    <owl:Ontology rdf:about="http://award.bibliotek-o.org/ontology/">
        <owl:ontologyIRI rdf:resource="http://award.bibliotek-o.org/ontology/"/>
        <owl:versionIRI rdf:resource="http://award.bibliotek-o.org/0.1/ontology/"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Version 0.1</owl:versionInfo>
        <dcterms:issued></dcterms:issued>
        <dcterms:modified></dcterms:modified>
        <rdfs:label xml:lang="en">Award Ontology</rdfs:label>
        <skos:definition xml:lang="en">The Award Ontology models awards and their recipients.</skos:definition>
        <vann:preferredNamespacePrefix>award</vann:preferredNamespacePrefix>
        <dcterms:title xml:lang="en">Award Ontology</dcterms:title>
        <!-- Omitting, since bibliotek-o formally extends BIBFRAME and the relationships are transitive
            <voaf:extends rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
            <voaf:specializes rdf:resource="http://id.loc.gov/ontologies/bibframe/"/>
        -->
        <voaf:extends rdf:resource="http://bibliotek-o.org/ontology/"/>
        <voaf:specializes rdf:resource="http://bibliotek-o.org/ontology/"/>
        <owl:imports rdf:resource="http://bibliotek-o.org/1.1/ontology/"/>
    </owl:Ontology>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardCitation -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardCitation">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is citation.</skos:definition>
        <rdfs:label>Award citation</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardGranterActivity -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardGranterActivity">
        <rdfs:subClassOf rdf:resource="http://bibliotek-o.org/1.1/ontology/Activity"/>
        <skos:definition>The activity of granting an award.</skos:definition>
        <skos:scopeNote>This class is not derived from a MARC relator.</skos:scopeNote>
        <rdfs:label>Award granter</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardHonoraryMention -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardHonoraryMention">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is honorary mention.</skos:definition>
        <rdfs:label>Award honorary mention</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardLonglist -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardLonglist">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is longlist.</skos:definition>
        <rdfs:label>Award Longlist</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardNominee -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardNominee">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is nominee.</skos:definition>
        <rdfs:label>Award nominee</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardReceipt -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt">
        <skos:definition>The bestowal of an award, honor, or distinction to an Agent or Work. The award bestowed should be represented with the Award class. The AwardReceipt is a context node between the award and the recipient which may contain further information, such as date.</skos:definition>
        <rdfs:label>Award receipt</rdfs:label>
    </owl:Class>
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardSelectorActivity -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardSelectorActivity">
        <rdfs:subClassOf rdf:resource="http://bibliotek-o.org/1.1/ontology/Activity"/>
        <skos:definition>The activity of nominating or judging a resource in relation to an award, honor, etc.</skos:definition>
        <skos:scopeNote>This class is not derived from a MARC relator.</skos:scopeNote>
        <rdfs:label>Award selector</rdfs:label>
    </owl:Class>
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardShortlist -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardShortlist">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is award shortlist.</skos:definition>
        <rdfs:label>Award shortlist</rdfs:label>
    </owl:Class>
    
    
    
    <!-- http://award.bibliotek-o.org/0.1/ontology/AwardWinner -->
    
    <owl:Class rdf:about="http://award.bibliotek-o.org/0.1/ontology/AwardWinner">
        <rdfs:subClassOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>The specific nature of the award receipt is award winner.</skos:definition>
        <rdfs:label>Award winner</rdfs:label>
    </owl:Class>
    
    
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://award.bibliotek-o.org/0.1/ontology/hasAward -->

    <owl:ObjectProperty rdf:about="http://award.bibliotek-o.org/0.1/ontology/hasAward">
        <owl:inverseOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/isAwardOf"/>
        <rdfs:domain rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <rdfs:range rdf:resource="http://vivoweb.org/ontology/core#Award"/>
        <skos:definition>This resource is a receipt of the specified award or honor.</skos:definition>
        <rdfs:label>has award</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://award.bibliotek-o.org/0.1/ontology/isAwardOf -->

    <owl:ObjectProperty rdf:about="http://award.bibliotek-o.org/0.1/ontology/isAwardOf">
        <rdfs:domain rdf:resource="http://vivoweb.org/ontology/core#Award"/>
        <rdfs:range rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>Links a specified award or honor to its receipt.</skos:definition>
        <rdfs:label>is award of</rdfs:label>
        <owl:inverseOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/hasAward"/>
    </owl:ObjectProperty>
    


    <!-- http://award.bibliotek-o.org/0.1/ontology/received -->

    <owl:ObjectProperty rdf:about="http://award.bibliotek-o.org/0.1/ontology/received">
        <owl:inverseOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/receivedBy"/>
        <rdfs:range rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>This resource, such as an Agent or Work, is the recipient of the specified AwardReceipt.</skos:definition>
        <rdfs:label>received</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://award.bibliotek-o.org/0.1/ontology/receivedBy -->

    <owl:ObjectProperty rdf:about="http://award.bibliotek-o.org/0.1/ontology/receivedBy">
        <owl:inverseOf rdf:resource="http://award.bibliotek-o.org/0.1/ontology/received"/>
        <rdfs:domain rdf:resource="http://award.bibliotek-o.org/0.1/ontology/AwardReceipt"/>
        <skos:definition>This AwardReceipt was received by the specified award recipient (such as an Agent or Work).</skos:definition>
        <rdfs:label>received by</rdfs:label>
    </owl:ObjectProperty>

</rdf:RDF>

