# NOTE!!!!!!! This pattern is only using http://measurement.bibliotek-o.org/ because custom forms in VitroLib began with this namespace before RareMat and ArtFrame decided on a different namespace. Anyone using this shape should use the new namespace. 

raremat_shapes_monograph:MeasurementGroupForm
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:label "Measurement Group Form" ;
  sh:description "A group of measurements, e.g. length, width, height measuring the size of a resource would be one measurement group" ;
  sh:targetClass <http://measurement.bibliotek-o.org/MeasurementGroup> ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property raremat_shapes_monograph:MeasurementGroupForm_dcterms_description ;
  sh:property raremat_shapes_monograph:MeasurementGroupForm_measurement_hasMeasurement .
  
  
raremat_shapes_monograph::MeasurementGroupForm_dcterms_description sh:path dcterms:description ;
      sh:description "Record any qualifying information about the measurement, e.g. 'approximate', 'or smaller', 'in container', 'with handle' " ;
      sh:name "Qualifying information about the measurement" ;
      sh:nodeKind sh:Literal ;
      sh:order "0"^^xsd:decimal .
    
raremat_shapes_monograph:MeasurementGroupForm_measurement_hasMeasurement
      sh:path <http://measurement.bibliotek-o.org/hasMeasurement> ;
      sh:class <http://measurement.bibliotek-o.org/Measurement> ;
      sh:description "The measurement of a single aspect of a resource, including value, units, and the dimension measured. For example, a book may have a height (aspect) of 10 (value) centimeters (units). Each such measurement is attached to a MeasurementGroup." ;
      sh:name "Single measurement" ;
      sh:nodeKind sh:IRI ;
      sh:order "1"^^xsd:decimal ;
      tosh:editWidget swa:NestedObjectEditor ..



